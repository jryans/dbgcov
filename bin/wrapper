#!/bin/bash

# set ourselves as the wrapper
WRAPPER="$0"
. "${WRAPPER_FUNCS:-${TOOLSUB:-$(dirname "$0")/../..}/wrapper/lib/wrapper-funcs.sh}"

CPP="$(dirname "$0")/dbgcov"

# delegate to the generic wrapper -- we've set WRAPPER so it won't re-source the funcs
. "${TOOLSUB:-$(dirname "$0")/../..}/wrapper/bin/wrapper"
